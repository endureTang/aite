<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nj.dao.extend.NjProdPayeeObjMapperExtend">
  
  <!-- 根据产品ID获取收款人信息 -->
  <select id="selectByProductId" parameterType="java.lang.String" resultType="com.nj.model.generate.NjProdPayeeObj">
  	SELECT * FROM nj_prod_payee_obj WHERE product_id = #{productId}
  </select>
</mapper>