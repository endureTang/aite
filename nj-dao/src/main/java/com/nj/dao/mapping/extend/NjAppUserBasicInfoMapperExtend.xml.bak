<!-- 张海添加，获取最近的联系人信息 -->
	<select id="getRecentBasicInfo" parameterType="java.lang.String"
		resultMap="BaseResultMap">
		SELECT * FROM nj_user_basic_info INNER JOIN
		(SELECT basic.user_id as user_id, max(version) as max_version FROM nj_user_basic_info basic 
			WHERE basic.user_id=#{userId,jdbcType=VARCHAR} GROUP BY basic.user_id) tmp
		ON  nj_user_basic_info.user_id= tmp.user_id AND nj_user_basic_info.version=tmp.max_version
	</select>

	<select id="getUserBasicInfoByOrderId" parameterType="java.lang.String"
		resultMap="BaseResultMap">
		SELECT nubi.* FROM nj_user_basic_info nubi 
			INNER JOIN nj_order_userinfo nou
			on nou.order_id = #{orderId,jdbcType=VARCHAR}
			and nou.basic_info_id = nubi.id
	</select>
