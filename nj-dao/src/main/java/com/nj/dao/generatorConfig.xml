<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="context1">
		<property name="javaFileEncoding" value="UTF-8"/> 
		<!-- 注释 --> 
		<commentGenerator type="com.jdcf.asset.dao.generator.MyCommentGenerator">
        </commentGenerator>  
		<jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/nj_server"
			driverClass="com.mysql.jdbc.Driver" password="root" userId="root">
			<!-- 针对mysql数据库 -->
            <!-- <property name="useInformationSchema" value="true"></property> -->
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="true" />
		</javaTypeResolver>
		
		<!-- 常规 -->
		<javaModelGenerator targetPackage="com.nj.model.generate" targetProject="nj-model">
			<!-- <property name="rootClass" value="com.nj.core.base.entity.NjIdEntity"/> -->
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="com.nj.dao.mapping" targetProject="nj-dao"/>
		<javaClientGenerator targetPackage="com.nj.dao" implementationPackage="com.nj.dao" targetProject="nj-dao" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
			<property name="rootInterface" value="com.nj.core.base.mapper.BaseMapper"/>
		</javaClientGenerator>
		
		<table tableName="nj_strategy">
			<property name="useActualColumnNames" value="false"/>
		</table>

		<!-- e签宝 -->
		<!-- <javaModelGenerator targetPackage="com.nj.model.esign.base" targetProject="nj-model" 
			/> <sqlMapGenerator targetPackage="com.nj.dao.esign.mapping.base" targetProject="nj-dao" 
			/> <javaClientGenerator targetPackage="com.nj.dao.esign.mapper.base" implementationPackage="com.nj.dao" 
			targetProject="nj-dao" type="XMLMAPPER"> <property name="enableSubPackages" 
			value="true" /> </javaClientGenerator> <table tableName="sys_operate_log" 
			/> -->
	</context>
</generatorConfiguration>