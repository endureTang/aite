<xml>
	<!--产品基本配置，模版部分必须配置-->
	<prodBasicInfo>
		<productName>production name</productName>
		<productDescription>房易贷</productDescription>
	</prodBasicInfo>
	<!--渠道配置,模版不需要配置-->
	<channelConf></channelConf>
	<!--产品业务配置，模版部分必须配置-->
	<businessConf>
        <!--资料设置-->
		<dataConfiguration>
			<!--身份认证-->
			<identityAuthentication>必填</identityAuthentication>
          	<!--个人资料-->
			<personalData>必填</personalData>
			<!--工作信息-->
			<jobInformation>必填</jobInformation>
			<!--联系人-->
			<contacts>必填</contacts>
			<!--车辆信息-->
			<carInfo>非必填</carInfo>
		</dataConfiguration>
		<!--征信设置-->
		<creditSetting>
			<!--身份核实-->
			<identityVerification>必填</identityVerification>
          	<!--芝麻信用-->
			<sesameCredit>必填</sesameCredit>
          	<!--通讯录授权-->
			<mailList>必填</mailList>
		</creditSetting>
		<!--贷款申请参数配置-->
		<loanAllocation>
			<!--房款额度最低-->
			<minimumLimit>1</minimumLimit>
          	<!--房款额度最高-->
			<maximumAmount>10</maximumAmount>
			<!--递增增幅-->
			<incrementalIncrease>千元</incrementalIncrease>
			<!--可选期限-->
			<optionalPeriod>
				<!--期数-->
				<item>3期</item>
				<item>6期</item>
				<item>9期</item>
			</optionalPeriod>
		</loanAllocation>
	</businessConf>
	<!--订单受理配置-->
	<orderConf>
		<!--角色列表-->
		<roleList>
			<item>busiaudit</item>
			<item>riskaudit</item>
			<item>loanofficer</item>
			<item>businessReception</item>
			<item>backgroundManager</item>
			<item>businessExecutive</item>
		</roleList>
	</orderConf>
	<!--发布配置-->
	<publishConf>
		<!--申请适用区域-->
		<applicationArea>
			<!--区域-->
			<item>成都市</item>
			<item>北京市</item>
		</applicationArea>
		<!--申请用户范围设置-->
		<applicationUserSettings>白名单用户</applicationUserSettings>
		<!--上线分类设置-->
		<onlineClassification>
			<item>消费</item>
			<item>信用</item>
		</onlineClassification>
	</publishConf>
</xml>